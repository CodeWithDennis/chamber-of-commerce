{
    "name": "codewithdennis/chamber-of-commerce",
    "description": "A PHP wrapper tailored for simplifying and enhancing interactions with the Chamber of Commerce API from The Netherlands.",
    "keywords": [
        "CodeWithDennis",
        "Laravel",
        "Chamber Of Commerce",
        "Kamer van Koophandel",
        "Nederland",
        "PHP",
        "Bedrijven",
        "Companies"
    ],
    "homepage": "https://github.com/codewithdennis/chamber-of-commerce",
    "license": "MIT",
    "authors": [
        {
            "name": "CodeWithDennis",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.8"
    },
    "autoload": {
        "psr-4": {
            "CodeWithDennis\\ChamberOfCommerce\\": "src/"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodeWithDennis\\ChamberOfCommerce\\ChamberOfCommerceServiceProvider"
            ],
            "aliases": {
                "ChamberOfCommerce": "CodeWithDennis\\ChamberOfCommerce\\Facades\\ChamberOfCommerce"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}