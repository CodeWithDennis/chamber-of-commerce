{
    "name": "codewithdennis/chamber-of-commerce",
    "description": "Chamber of Commerce API wrapper for Laravel designed to simplify and streamline interactions with the Dutch Chamber of Commerce API.",
    "keywords": [
        "CodeWithDennis",
        "laravel",
        "chamber-of-commerce"
    ],
    "homepage": "https://github.com/codewithdennis/chamber-of-commerce",
    "license": "MIT",
    "authors": [
        {
            "name": "CodeWithDennis",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.8"
    },
    "autoload": {
        "psr-4": {
            "CodeWithDennis\\ChamberOfCommerce\\": "src/"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodeWithDennis\\ChamberOfCommerce\\ChamberOfCommerceServiceProvider"
            ],
            "aliases": {
                "ChamberOfCommerce": "CodeWithDennis\\ChamberOfCommerce\\Facades\\ChamberOfCommerce"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}